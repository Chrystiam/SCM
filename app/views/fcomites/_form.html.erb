<%= simple_form_for @fcomite, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :fecha %>
  <%= f.input :fecha, :as => :string, :label => false, :input_html => {  :class=>"datepicker" } %>
  <%= f.input :lugar %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                fcomites_path, :class => 'btn' %>
  </div>
<% end %>
