<tr id=<%= "queja_#{queja.id}" %>>
  
  <td><%= queja.id %></td>
  <td><%= queja.created_at %></td>
  <td><%= queja.nombres %></td>
  <td><%= queja.apellidos %></td>
  <td><%= queja.tipo_documento.descripcion %> </td>
  <td><%= queja.identificacion %></td>
  <td><%= queja.falta.descripcion %></td>
  <td><%= queja.estado %></td>
  
  <td>
    
    <%= link_to 'Mostrar', queja, :remote => true, :class => 'btn btn-mini btn-default' %>
    <%= link_to 'Editar', edit_queja_path(queja), :remote => true,
        :class => 'btn btn-mini btn-warning' %>
    <%= link_to 'Eliminar', queja, 
        :data => { :confirm => 'Esta Seguro?' },
        :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
  </td>
</tr>


