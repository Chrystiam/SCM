<%- model_class = Asignacioncomite -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombres) %>:</strong></dt>
  <dd><%= @asignacioncomite.nombres %></dd>
  <dt><strong><%= model_class.human_attribute_name(:apellidos) %>:</strong></dt>
  <dd><%= @asignacioncomite.apellidos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:programa) %>:</strong></dt>
  <dd><%= @asignacioncomite.programa %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ficha) %>:</strong></dt>
  <dd><%= @asignacioncomite.ficha %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              asignacioncomites_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_asignacioncomite_path(@asignacioncomite), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              asignacioncomite_path(@asignacioncomite),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
